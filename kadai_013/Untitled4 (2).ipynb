      "source": [
        "def add_tax_to_price(price, tax_rate):\n",
        "    tax_amount = price * (tax_rate / 100)\n",
        "    total_price = price + tax_amount\n",
        "    return round(total_price, 2)\n",
        "\n",
        "price = 110\n",
        "tax_rate = 10\n",
        "print(add_tax_to_price(price, tax_rate))"
