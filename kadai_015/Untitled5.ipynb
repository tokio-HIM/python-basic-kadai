        "class Human:\n",
        "    def __init__(self, name, age):\n",
        "      self.name = name\n",
        "      self.age = age\n",
        "\n",
        "    def printinfo(self):\n",
        "      print(f\"Name: {self.name}, Age: {self.age}\")\n",
        "\n",
        "person = Human(\"Taro\", 25)\n",
        "\n",
        "person.printinfo()\n",
